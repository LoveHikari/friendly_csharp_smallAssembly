<Window x:Class="DeveloperKit.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DeveloperKit"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Menu HorizontalAlignment="Left" Height="20" VerticalAlignment="Top" Width="100">
            <MenuItem Header="工具箱">
                <MenuItem Header="JAVA环境变量配置" Command="{Binding JavaConfigCommand}"></MenuItem>
                <MenuItem Header="代码生成器" Command="{Binding CreateCodeCommand}"></MenuItem>
            </MenuItem>
        </Menu>
    </Grid>
</Window>
